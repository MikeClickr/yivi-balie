<IssueSpecification version="4">
	<Name>
		<en>Demo Travel Document</en>
		<nl>Demo Reisdocument</nl>
	</Name>
	<ShortName>
		<en>Demo Travel Document</en>
		<nl>Demo Reisdocument</nl>
	</ShortName>
	<SchemeManager>tweedegolf-demo</SchemeManager>
	<IssuerID>amsterdam</IssuerID>
	<CredentialID>travelDocument</CredentialID>
	<Description>
		<en>Your Dutch passport or identity card</en>
		<nl>Uw Nederlandse paspoort of identiteitskaart, uitgelezen via de IRMA balie</nl>
	</Description>
	<ShouldBeSingleton>false</ShouldBeSingleton>

	<Attributes>
		<Attribute id="kind">
			<Name>
				<en>Document kind</en>
				<nl>Soort document</nl>
			</Name>
			<Description>
				<en>The document kind (passport or identity card)</en>
				<nl>Het soort reisdocument (paspoort of identiteitskaart)</nl>
			</Description>
		</Attribute>
		<Attribute id="number">
			<Name>
				<en>Document number</en>
				<nl>Documentnummer</nl>
			</Name>
			<Description>
				<en>The travel document number</en>
				<nl>Het documentnummer</nl>
			</Description>
		</Attribute>
		<Attribute id="dateofexpiry">
			<Name>
				<en>Expiry date</en>
				<nl>Verloopdatum</nl>
			</Name>
			<Description>
				<en>Expiry date of the travel document</en>
				<nl>Verloopdatum van het reisdocument</nl>
			</Description>
		</Attribute>
		<Attribute id="documentissuer" optional="true">
			<Name>
				<en>Document issuer</en>
				<nl>Uitgever</nl>
			</Name>
			<Description>
				<en>Issuer of the travel document</en>
				<nl>Autoriteit van afgifte reisdocument</nl>
			</Description>
		</Attribute>
		<Attribute id="gender">
			<Name>
				<en>Gender</en>
				<nl>Geslacht</nl>
			</Name>
			<Description>
				<en>Your gender</en>
				<nl>Je geslacht</nl>
			</Description>
		</Attribute>
		<Attribute id="surname">
			<Name>
				<en>Surname</en>
				<nl>Achternaam</nl>
			</Name>
			<Description>
				<en>Your full family name: your family name or (a combination with) the name of your partner</en>
				<nl>Je volledige achternaam: je geslachtsnaam of (een combinatie met) de naam van je partner</nl>
			</Description>
		</Attribute>
		<Attribute id="dateofbirth">
			<Name>
				<en>Date of birth</en>
				<nl>Geboortedatum</nl>
			</Name>
			<Description>
				<en>Your date of birth</en>
				<nl>Je geboortedatum</nl>
			</Description>
		</Attribute>
		<Attribute id="nationality">
			<Name>
				<en>Nationality</en>
				<nl>Nationaliteit</nl>
			</Name>
			<Description>
				<en>Your nationality</en>
				<nl>Je nationaliteit</nl>
			</Description>
		</Attribute>
		<Attribute id="over12">
			<Name>
				<en>Over 12</en>
				<nl>Ouder dan 12</nl>
			</Name>
			<Description>
				<en>Whether you are over 12</en>
				<nl>Of je ouder dan 12 bent</nl>
			</Description>
		</Attribute>
		<Attribute id="over16">
			<Name>
				<en>Over 16</en>
				<nl>Ouder dan 16</nl>
			</Name>
			<Description>
				<en>Whether you are over 16</en>
				<nl>Of je ouder dan 16 bent</nl>
			</Description>
		</Attribute>
		<Attribute id="over18">
			<Name>
				<en>Over 18</en>
				<nl>Ouder dan 18</nl>
			</Name>
			<Description>
				<en>Whether you are over 18</en>
				<nl>Of je ouder dan 18 bent</nl>
			</Description>
		</Attribute>
		<Attribute id="over21">
			<Name>
				<en>Over 21</en>
				<nl>Ouder dan 21</nl>
			</Name>
			<Description>
				<en>Whether you are over 21</en>
				<nl>Of je ouder dan 21 bent</nl>
			</Description>
		</Attribute>
		<Attribute id="over65">
			<Name>
				<en>Over 65</en>
				<nl>Ouder dan 65</nl>
			</Name>
			<Description>
				<en>Whether you are over 65</en>
				<nl>Of je ouder dan 65 bent</nl>
			</Description>
		</Attribute>
		<Attribute id="personalnumber">
			<Name>
				<en>Personal Identification Number</en>
				<nl>Persoonlijk identificatienummer</nl>
			</Name>
			<Description>
				<en>Your Personal Identification Number. For Dutch travel documents your Dutch Citizen service number (BSN)</en>
				<nl>Je persoonlijk identificatienummber. Voor Nederlandse reisdocumenten je Burgerservicenummer (BSN)</nl>
			</Description>
		</Attribute>
		<Attribute id="photo" displayHint="portraitPhoto">
			<Name>
				<en>Photo</en>
				<nl>Foto</nl>
			</Name>
			<Description>
				<en>The portrait photo as included in your travel document</en>
				<nl>De portretfoto zoals deze bij uw reisdocument is gevoegd</nl>
			</Description>
		</Attribute>
	</Attributes>
</IssueSpecification>
